		<set-payload value='#["welcome to github"]' doc:name="Set Payload" doc:id="53bd5e4b-1a4e-4260-8672-7b8f770f9649" />
		<logger level="INFO" doc:name="Logger" doc:id="7daf4e52-4679-4f94-b76f-21d1622f7470" message="#[payload]"/>
	</flow>
</mule>
